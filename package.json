  "crawlingTarget": "https://baseconnect.in/",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.5",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "selenium-webdriver": "^4.1.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.0",
    "web-vitals": "^2.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "react-start": "craco start",
    "react-build": "craco build",
    "electron-build": "tsc ./electron/main.ts && webpack --config ./webpack.config.electron.js",
    "start": "yarn electron-build && concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron ./public/main.js\"",
    "build": "npm run react-build && electron-builder",
    "test": "craco test",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@types/eslint": "^8.4.1",
    "babel-loader": "^8.2.3",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
